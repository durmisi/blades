  "scripts": {
    "start": "concurrently \"npm run build-dev \" \"webpack-dev-server --hot --inline --port 8080\" \"dotnet watch run ASPNETCORE_ENVIRONMENT=Development\"",
    "build-dev": "concurrently \"set NODE_ENV=development && webpack\" \"dotnet build\" ",
    "build-production": "concurrently \"set NODE_ENV=production && webpack\" \"dotnet build\" ",
    "watch-dev": "set NODE_ENV=development && webpack --watch --color",
    "watch-production": "npm run build-production --watch --color",
    "publish-for-iis": "npm run build-production && dotnet publish -c Release",
    "test": "karma start",
    "vendorjs":"webpack --config webpack.config.vendor.js"
  },
